{
  "name": "sportunity",
  "description": "package withdev tools to transpile code into the sportunity package",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "node ./scripts/android-release-gradle-fix.js",
    "build:ios": "react-native bundle --assets-dest ./ --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "relay": "node_modules/relay-compiler/bin/relay-compiler --src ./src --schema conf/schema.json",
    "relay:watch": "yarn relay -- --watch",
    "clear": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "download_schema": "node scripts/saveSchema.js",
    "lint": "eslint src",
    "start": "react-native start",
    "test": "npm run-script --silent lint",
    "flow": "flow",
    "create-apk": "react-native bundle --platform android --dev false --entry-file index.android.js   --bundle-output android/app/src/main/assets/index.android.bundle   --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleRelease && cd ..",
    "create-dev-apk": "react-native bundle --platform android --dev true --entry-file index.android.js   --bundle-output android/app/src/main/assets/index.android.bundle   --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleReleaseStaging && cd ..",
    "push-dev": "appcenter codepush release-react -a contact-142/Sportunity-1 -m -d Staging && appcenter codepush release-react -a contact-142/Sportunity -m -d Staging",
    "push-dev-android": "appcenter codepush release-react -a contact-142/Sportunity-1 -m -d Staging",
    "push-dev-ios": "appcenter codepush release-react -a contact-142/Sportunity -m -d Staging",
    "push-prod-android": "appcenter codepush release-react -a contact-142/Sportunity-1 -m -d Production",
    "push-prod-ios": "appcenter codepush release-react -a contact-142/Sportunity -m -d Production"
  },
